{
  "name": "@nodite-light/admin-api",
  "version": "0.0.1",
  "private": true,
  "main": "src/server.ts",
  "engines": {
    "node": ">=8.10.0"
  },
  "scripts": {
    "build": "tsoa spec-and-routes && tsc && tsc-alias",
    "start:prod": "node dist/server.js",
    "start:dev": "concurrently \"nodemon\" \"npm run server:dev\"",
    "server:dev": "ts-node-dev --ignore-watch dist,node_modules,.tsoa --transpile-only --inspect=0.0.0.0:9229 --exit-child --respawn -r tsconfig-paths/register src/server.ts",
    "test": "jest -c jest.config.js --env=node",
    "test:cov": "jest -c jest.config.js --coverage --env=node",
    "test:watch": "jest -c jest.config.js -i --watchAll",
    "lint": "eslint ./src/**/*.ts",
    "lint:fix": "eslint **/*.ts --fix",
    "prettier": "prettier --check \"{*.js,!(node*)**/*.ts}\"",
    "prettier:fix": "prettier --write \"{*.js,!(node*)**/*.ts}\"",
    "loglevel:change": "runtime-node-refresh",
    "tsoa": "tsoa spec-and-routes"
  },
  "author": "Oscaner Miao <oscaner1997@gmail.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "apicache": "^1.6.3",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.1",
    "express": "^4.17.1",
    "express-http-context": "^1.2.4",
    "express-rate-limit": "^7.1.5",
    "express-unless": "^2.1.3",
    "fs": "^0.0.1-security",
    "glob": "^10.3.10",
    "helmet": "^7.1.0",
    "http-status": "^1.5.0",
    "joi": "^17.5.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "morgan": "^1.10.0",
    "mysql2": "^3.6.5",
    "node-mocks-http": "^1.11.0",
    "path": "^0.12.7",
    "runtime-node-refresh": "^1.0.6",
    "sequelize": "^6.35.1",
    "supertest": "^6.1.6",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^5.1.1",
    "typescript": "^5.3.2",
    "uuid": "^9.0.0",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@types/apicache": "^1.6.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.10",
    "@types/morgan": "^1.9.3",
    "@types/node": "^20.10.2",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-security": "^1.7.1",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.5.0",
    "tsconfig-paths": "^4.2.0"
  }
}
